<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<plugin	label="plugins/ExportPdf">
	<plugin_action 
		documentation="%plugins/ExportPdf.xml_documentation" 
		name="%plugins/ExportPdf.xml_name" base="freemind.extensions.ModeControllerHookAdapter" 
		label="plugins/ExportPdf.xml" 
		class_name="plugins.svg.ExportPdf">
		<plugin_mode class_name="freemind.modes.mindmapmode"/>
		<plugin_menu location="menu_bar/file/export/pdf/pdf_export"/>
		<plugin_property name="export_type" value="map"/>
	</plugin_action>
	<plugin_action 
		documentation="%plugins/ExportPdfNode.xml_documentation" 
		name="%plugins/ExportPdfNode.xml_name" base="freemind.extensions.ModeControllerHookAdapter" 
		label="plugins/ExportPdfNode.xml" 
		class_name="plugins.svg.ExportPdf">
		<plugin_mode class_name="freemind.modes.mindmapmode"/>
		<plugin_menu location="menu_bar/file/export/pdf/pdf_branch_export"/>
		<plugin_property name="export_type" value="node"/>
	</plugin_action>
	<plugin_action 
		documentation="%plugins/ExportSvg.xml_documentation" 
		name="%plugins/ExportSvg.xml_name" base="freemind.extensions.ModeControllerHookAdapter" 
		label="plugins/ExportSvg.xml" 
		class_name="plugins.svg.ExportSvg">
		<plugin_mode class_name="freemind.modes.mindmapmode"/>
		<plugin_menu location="menu_bar/file/export/pdf/svg_export"/>
	</plugin_action>
</plugin>
